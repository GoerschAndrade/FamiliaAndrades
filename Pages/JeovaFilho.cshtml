@page
@model IndexModel
@{
    ViewData["Title"] = "Família de José Jeová Andrade Filho";
}

<div id="myCarousel" class="carousel slide" data-ride="carousel" data-interval="6000">
    <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
        <li data-target="#myCarousel" data-slide-to="3"></li>
        <li data-target="#myCarousel" data-slide-to="4"></li>>
    </ol>
    <div class="carousel-inner" role="listbox">
        <div class="item active">
            <img src="~/images/FamiliaJeovaFilho.svg" />
            <div class="carousel-caption" role="option">
                <p>
                   José Jeová Andrade Filho e família.
                </p>
            </div>
        </div>
        <div class="item">
            <img src="~/images/FamiliaJeovaFilho.svg" alt="Visual Studio" class="img-responsive" />
            <div class="carousel-caption" role="option">
                <p>
                    José Jeová Andrade Neto.
                <p>
                   <a class="btn btn-default" asp-page="/JeovaFilho">
                   Saiba mais.
                   </a>
                </p>
            </div>
        </div>

        <div class="item">
            <img src="~/images/FamiliaJeovaFilho.svg" alt="Visual Studio" class="img-responsive" />
            <div class="carousel-caption" role="option">
                <p>
                   Ana Karisse de Carvalho Andrade.
                <p>   
                    <a class="btn btn-default" asp-page="/JeovaFilho">
                   Saiba mais.
                   </a>
                </p>
            </div>
        </div>
        <div class="item">
            <img src="~/images/FamiliaJeovaElzaGenros.svg" alt="Visual Studio" class="img-responsive" />
            <div class="carousel-caption" role="option">
                <p>
                   José Jeová Andrade e família.
                <p>   
                   <a class="btn btn-default" asp-page="/Jeova">
                   Saiba mais.
                   </a>
                </p>
            </div>
        </div>
        <div class="item">
            <img src="~/images/FamiliaPatriarca.svg" alt="Visual Studio" class="img-responsive" />
            <div class="carousel-caption" role="option">
                <p>
                   Raimundo Gomnes Andrade e família.
                <p>   
                   <a class="btn btn-default" asp-page="/Index">
                   Saiba mais.
                   </a>
                </p>
            </div>
        </div>
    </div>
    <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>

